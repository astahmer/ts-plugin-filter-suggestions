{
	"name": "vscode-ts-plugin-filter-suggestions",
	"publisher": "astahmer",
	"description": "A TypeScript Language Service Plugin that allows configuring filters for suggestions",
	"displayName": "ts-plugin-filter-suggestions",
	"version": "0.0.1",
	"private": true,
	"main": "./out/extension.js",
	"repository": {
		"url": "https://github.com/astahmer/ts-plugin-filter-suggestions"
	},
	"engines": {
		"vscode": "^1.95.0"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"typescriptServerPlugins": [
			{
				"name": "ts-plugin-filter-suggestions",
				"enableForWorkspaceTypeScriptVersions": true
			}
		]
	},
	"files": [
		"out",
		"package.json",
		"LICENSE",
		"assets",
		"README.md"
	],
	"scripts": {
		"vscode:prepublish": "pnpm run build",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "pnpm vsce package --allow-unused-files-pattern",
		"vsce:publish": "pnpm vsce publish --allow-unused-files-pattern"
	},
	"dependencies": {
		"ts-plugin-filter-suggestions": "link:.."
	},
	"devDependencies": {
		"@types/vscode": "^1.95.0",
		"@vscode/vsce": "^3.2.1",
		"typescript": "^5.7.3"
	},
	"__icon": "assets/logo.png",
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"ts",
		"plugin",
		"filter",
		"suggestions",
		"intellisense",
		"remove",
		"limit"
	],
	"license": "MIT"
}
