{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"files": {
		"ignoreUnknown": false,
		"ignore": [
			"**/node_modules/**",
			"**/dist/**",
			"**/migrations/**",
			"**/routeTree.gen.ts"
		]
	},
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": false
	}
}
